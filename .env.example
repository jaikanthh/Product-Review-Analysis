# Google AI API Configuration
# Copy this file to .env and add your actual API keys

# Google Gemini Pro API Key
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_GEMINI_API_KEY=your_gemini_api_key_here

# Google Vertex AI Configuration
# Get from: https://console.cloud.google.com/
GOOGLE_VERTEX_AI_PROJECT_ID=your_project_id_here
GOOGLE_VERTEX_AI_LOCATION=us-central1

# Google Natural Language API Key
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_NATURAL_LANGUAGE_API_KEY=your_natural_language_api_key_here

# Google Translate API Key
# Get from: https://console.cloud.google.com/apis/credentials
GOOGLE_TRANSLATE_API_KEY=your_translate_api_key_here

# Database Configuration
DATABASE_URL=sqlite:///data/warehouse/reviews.db

# Other Configuration
DEBUG=True
LOG_LEVEL=INFO